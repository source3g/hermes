<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/jsp/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/cxf/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/login.htm</pattern>
		<pattern> /login/*</pattern>
		<pattern> /js/*</pattern>
		<pattern> /img/*		</pattern>
		<pattern>/css/*</pattern>
		<pattern>/jsp/admin/</pattern>
		<pattern>/device/*</pattern>
		<pattern>/api/*</pattern>
	</excludes>

	<decorator name="admin" page="adminDefault.jsp">
		<pattern>/admin/*</pattern>
	</decorator>

	<decorator name="default" page="default.jsp">
		<pattern>/merchant/*</pattern>
	</decorator>


</decorators>
